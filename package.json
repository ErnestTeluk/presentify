{
  "name": "presentify",
  "version": "0.0.1",
  "private": true,
  "author": "Ernest Teluk",
  "scripts": {
    "build": "tsc --build --incremental tsconfig.build.json",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "lint:code": "eslint --cache --cache-location node_modules/.cache/eslint --cache-strategy content --ext ts,tsx .",
    "lint:types": "npm run build"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --config .prettierrc.js --write",
      "eslint --fix",
      "npm run lint:types"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/styled-components": "5.1.26",
    "@vitest/coverage-istanbul": "0.29.3",
    "eslint-config-vazco": "7.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.5.3",
    "husky": "8.0.0",
    "jsdom": "21.1.1",
    "lerna": "6.0.1",
    "lint-staged": "13.2.0",
    "vitest": "0.29.2"
  }
}
